<%= form_for :post do |f| %>
<div>
  <div class="input-field">
    <%= f.label :title %>
    <br>
    <%= f.text_field :title %>
    <br>
  </div>
  <div class="input-field">
    <%= f.label :content %>
    <br>
    <%= f.text_field :content %>
    <br>
  </div>

  <button class='btn' action='<%= f.submit  %>'>Create Entry
  </button>
  <% end %>
</div>
